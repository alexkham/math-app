    {
        "tableTitle": "Derivative of Inverse Trigonometric Functions",
        "rows": [
          {
            "formula_title": "Derivative of Inverse Sine (arcsin)",
            "description": "Derivative of the arcsin function",
            "formula": "\\frac{d}{dx} \\arcsin(x) = \\frac{1}{\\sqrt{1 - x^2}}",
            "key": "Valid for |x| < 1"
          },
          {
            "formula_title": "Derivative of Inverse Cosine (arccos)",
            "description": "Derivative of the arccos function",
            "formula": "\\frac{d}{dx} \\arccos(x) = -\\frac{1}{\\sqrt{1 - x^2}}",
            "key": "Valid for |x| < 1"
          },
          {
            "formula_title": "Derivative of Inverse Tangent (arctan)",
            "description": "Derivative of the arctan function",
            "formula": "\\frac{d}{dx} \\arctan(x) = \\frac{1}{1 + x^2}",
            "key": ""
          },
          {
            "formula_title": "Derivative of Inverse Cotangent (arccot)",
            "description": "Derivative of the arccot function",
            "formula": "\\frac{d}{dx} \\text{arccot}(x) = -\\frac{1}{1 + x^2}",
            "key": ""
          },
          {
            "formula_title": "Derivative of Inverse Secant (arcsec)",
            "description": "Derivative of the arcsec function",
            "formula": "\\frac{d}{dx} \\text{arcsec}(x) = \\frac{1}{|x| \\sqrt{x^2 - 1}}",
            "key": "Valid for |x| > 1"
          },
          {
            "formula_title": "Derivative of Inverse Cosecant (arccsc)",
            "description": "Derivative of the arccsc function",
            "formula": "\\frac{d}{dx} \\text{arccsc}(x) = -\\frac{1}{|x| \\sqrt{x^2 - 1}}",
            "key": "Valid for |x| > 1"
          }
        ]
      },
    {
      "tableTitle": "Exponential and Logarithmic Functions",
      "rows": [
        {
          "formula_title": "Derivative of e^x",
          "description": "Derivative of the exponential function",
          "formula": "\\frac{d}{dx} e^x = e^x",
          "key": "e: Euler's number"
        },
        {
          "formula_title": "Derivative of ln(x)",
          "description": "Derivative of the natural logarithm",
          "formula": "\\frac{d}{dx} \\ln(x) = \\frac{1}{x}",
          "key": "ln: natural logarithm"
        },
        {
          "formula_title": "Derivative of a^x",
          "description": "Derivative of a to the power x",
          "formula": "\\frac{d}{dx} a^x = a^x \\ln(a)",
          "key": "a: base of the exponential function"
        },
        {
          "formula_title": "Derivative of log_a(x)",
          "description": "Derivative of logarithm base a",
          "formula": "\\frac{d}{dx} \\log_a(x) = \\frac{1}{x \\ln(a)}",
          "key": "a: base of the logarithm"
        }
      ]
    },
    {
      "tableTitle": "Product and Quotient Rules",
      "rows": [
        {
          "formula_title": "Product Rule",
          "description": "Derivative of the product of two functions",
          "formula": "\\frac{d}{dx} [f(x)g(x)] = f'(x)g(x) + f(x)g'(x)"
          
        },
        {
          "formula_title": "Quotient Rule",
          "description": "Derivative of the quotient of two functions",
          "formula": "\\frac{d}{dx} \\left[\\frac{f(x)}{g(x)}\\right] = \\frac{f'(x)g(x) - f(x)g'(x)}{g(x)^2}"
          
        }
      ]
    },
