      "tableTitle": "Basic Derivative Rules",
      "rows": [
        {
          "formula_title": "Power Rule",
          "description": "Derivative of a power of x",
          "formula": "\\frac{d}{dx} x^n = nx^{n-1}",
          "key": "n: any real number"
        },
        {
          "formula_title": "Constant Rule",
          "description": "Derivative of a constant",
          "formula": "\\frac{d}{dx} c = 0",
          "key": "c: constant value"
        },
        {
          "formula_title": "Constant Multiple Rule",
          "description": "Derivative of a constant times a function",
          "formula": "\\frac{d}{dx} [cf(x)] = c \\cdot f'(x)",
          "key": "c: constant value"
        },
        {
          "formula_title": "Sum/Difference Rule",
          "description": "Derivative of the sum or difference of two functions",
          "formula": "\\frac{d}{dx} [f(x) \\pm g(x)] = f'(x) \\pm g'(x)",
          "key": ""
        }
      ]
    },
    {
      "tableTitle": "Trigonometric Functions",
      "rows": [
        {
          "formula_title": "Derivative of Sine",
          "description": "Derivative of the sine function",
          "formula": "\\frac{d}{dx} \\sin(x) = \\cos(x)",
          "key": ""
        },
        {
          "formula_title": "Derivative of Cosine",
          "description": "Derivative of the cosine function",
          "formula": "\\frac{d}{dx} \\cos(x) = -\\sin(x)",
          "key": ""
        },
        {
          "formula_title": "Derivative of Tangent",
          "description": "Derivative of the tangent function",
          "formula": "\\frac{d}{dx} \\tan(x) = \\sec^2(x)",
          "key": ""
        },
        {
            "formula_title": "Derivative of Cotangent",
            "description": "Derivative of the cotangent function",
            "formula": "\\frac{d}{dx} \\cot(x) = -\\csc^2(x)",
            "key": ""
          },
          {
            "formula_title": "Derivative of Secant",
            "description": "Derivative of the secant function",
            "formula": "\\frac{d}{dx} \\sec(x) = \\sec(x)\\tan(x)",
            "key": ""
          },
          {
            "formula_title": "Derivative of Cosecant",
            "description": "Derivative of the cosecant function",
            "formula": "\\frac{d}{dx} \\csc(x) = -\\csc(x)\\cot(x)",
            "key": ""
          }
          
      ]
