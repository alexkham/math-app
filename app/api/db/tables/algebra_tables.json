[

{
    "tableTitle": "Radical Expressions Formulas",
    "rows": [
      {
        "formula_title": "Basic  Square  Root",
        "description": "The square root of a number.",
        "formula": "\\sqrt{a}",
        "key": "a: radicand"
      },
      {
        "formula_title": "Nth Root",
        "description": "The nth root of a number.",
        "formula": "\\sqrt[n]{a}",
        "key": "a: radicand, n: degree of the root"
      },
      {
        "formula_title": "Properties of Nth Roots and Exponents",
        "description": "Different equivalent expressions involving nth roots and exponents.",
        "formula": "\\sqrt[n]{a^m} = (a^m)^{\\frac{1}{n}} = a^{\\frac{m}{n}} = (a^{\\frac{1}{n}})^m = (\\sqrt[n]{a})^m",
        "key": "a: base, m: exponent, n: degree of the root"
      },
      {
        "formula_title": "Product Rule for Square Roots",
        "description": "The square root of a product.",
        "formula": "\\sqrt{ab} = \\sqrt{a} \\cdot \\sqrt{b}",
        "key": "a, b: radicands"
      },
      {
        "formula_title": "Product Rule for Nth Roots",
        "description": "The nth root of a product.",
        "formula": "\\sqrt[n]{ab} = \\sqrt[n]{a} \\cdot \\sqrt[n]{b}",
        "key": "a, b: radicands, n: degree of the root"
      },
      {
        "formula_title": "Quotient Rule for Square Roots",
        "description": "The square root of a quotient.",
        "formula": "\\sqrt{\\frac{a}{b}} = \\frac{\\sqrt{a}}{\\sqrt{b}}",
        "key": "a: numerator, b: denominator"
      },
      {
        "formula_title": "Quotient Rule for Nth Roots",
        "description": "The nth root of a quotient.",
        "formula": "\\sqrt[n]{\\frac{a}{b}} = \\frac{\\sqrt[n]{a}}{\\sqrt[n]{b}}",
        "key": "a: numerator, b: denominator, n: degree of the root"
      },
      {
        "formula_title": "Rationalizing the Denominator",
        "description": "Eliminating the radical in the denominator.",
        "formula": "\\frac{a}{\\sqrt{b}} = \\frac{a\\sqrt{b}}{b}",
        "key": "a: numerator, b: radicand in the denominator"
      },
      {
        "formula_title": "Distributive Property over Addition",
        "description": "Distributing a radical over an addition.",
        "formula": "\\sqrt{a + b} \\neq \\sqrt{a} + \\sqrt{b}",
        "key": "a, b: terms inside the radical"
      },
      {
        "formula_title": "Distributive Property over Subtraction",
        "description": "Distributing a radical over a subtraction.",
        "formula": "\\sqrt{a - b} \\neq \\sqrt{a} - \\sqrt{b}",
        "key": "a, b: terms inside the radical"
      },
      {
        "formula_title": "Rational Exponent Representation",
        "description": "Representing radicals using rational exponents.",
        "formula": "a^{\\frac{1}{n}} = \\sqrt[n]{a}",
        "key": "a: base, n: degree of the root"
      },
      {
        "formula_title": "Fractional Exponent Multiplication",
        "description": "Multiplying numbers with fractional exponents.",
        "formula": "a^{\\frac{m}{n}} \\cdot b^{\\frac{m}{n}} = (ab)^{\\frac{m}{n}}",
        "key": "a, b: bases, m, n: parts of the exponent"
      },
      {
        "formula_title": "Fractional Exponent Division",
        "description": "Dividing numbers with fractional exponents.",
        "formula": "a^{\\frac{m}{n}} / b^{\\frac{m}{n}} = \\left(\\frac{a}{b}\\right)^{\\frac{m}{n}}",
        "key": "a, b: bases, m, n: parts of the exponent"
      },
      {
        "formula_title": "Power Rule for Radicals",
        "description": "Raising a radical to a power.",
        "formula": "\\left(\\sqrt[n]{a}\\right)^m = \\sqrt[n]{a^m}",
        "key": "a: radicand, m: exponent, n: degree of the root"
      },
      {
        "formula_title": "Rationalizing Complex Denominators",
        "description": "Rationalizing denominators with a binomial.",
        "formula": "\\frac{a}{\\sqrt{b} + \\sqrt{c}} = \\frac{a(\\sqrt{b} - \\sqrt{c})}{b - c}",
        "key": "a: numerator, b, c: radicands in the denominator"
      },
      {
        "formula_title": "Simplifying Radicals",
        "description": "Reducing radicals to simplest form.",
        "formula": "\\sqrt[n]{a^m} = \\sqrt[n]{a}^{\\frac{m}{n}}",
        "key": "a: radicand, m: exponent, n: degree of the root"
      },
      {
        "formula_title": "Conjugate Multiplication",
        "description": "Multiplying a radical expression by its conjugate.",
        "formula": "(\\sqrt{a} + \\sqrt{b})(\\sqrt{a} - \\sqrt{b}) = a - b",
        "key": "a, b: radicands"
      },
      {
        "formula_title": "Nth Root of a Product (Detailed)",
        "description": "The nth root of a product of two numbers, detailed representation.",
        "formula": "\\sqrt[n]{ab} = \\sqrt[n]{a} \\cdot \\sqrt[n]{b}",
        "key": "a, b: radicands, n: degree of the root"
      },
      {
        "formula_title": "Nth Root of a Quotient (Detailed)",
        "description": "The nth root of a quotient of two numbers, detailed representation.",
        "formula": "\\sqrt[n]{\\frac{a}{b}} = \\frac{\\sqrt[n]{a}}{\\sqrt[n]{b}}",
        "key": "a: numerator, b: denominator, n: degree of the root"
      }
    ]
  },
  {
    "tableTitle": "Exponential Formulas",
    "rows": [
      {
        "formula_title": "Zero Exponent Rule",
        "description": "Any number raised to the power of zero.",
        "formula": "a^0 = 1",
        "key": "a: base"
      },
      {
        "formula_title": "Identity Rule",
        "description": "Any number raised to the power of one.",
        "formula": "a^1 = a",
        "key": "a: base"
      },
      {
        "formula_title": "Product Rule",
        "description": "Product of same bases with different exponents.",
        "formula": "a^m \\times a^n = a^{m+n}",
        "key": "a: base, m, n: exponents"
      },
      {
        "formula_title": "Quotient Rule",
        "description": "Quotient of same bases with different exponents.",
        "formula": "\\frac{a^m}{a^n} = a^{m-n}",
        "key": "a: base, m: numerator exponent, n: denominator exponent"
      },
      {
        "formula_title": "Negative Exponent Rule",
        "description": "Number raised to a negative exponent.",
        "formula": "a^{-m} = \\frac{1}{a^m}",
        "key": "a: base, m: exponent"
      },
      {
        "formula_title": "Power of a Power Rule",
        "description": "Raising an exponent to another exponent.",
        "formula": "(a^m)^n = a^{mn}",
        "key": "a: base, m, n: exponents"
      },
      {
        "formula_title": "Power of a Product Rule",
        "description": "Raising a product to an exponent.",
        "formula": "(ab)^m = a^m b^m",
        "key": "a, b: bases, m: exponent"
      },
      {
        "formula_title": "Power of a Quotient Rule",
        "description": "Raising a quotient to an exponent.",
        "formula": "\\left(\\frac{a}{b}\\right)^m = \\frac{a^m}{b^m}",
        "key": "a: numerator, b: denominator, m: exponent"
      },
      {
        "formula_title": "Fractional Exponent Rule (x/y)",
        "description": "Expresses a power as a fractional exponent.",
        "formula": "a^{\\frac{x}{y}} = \\sqrt[y]{a^x}",
        "key": "a: base, x: exponent, y: root"
      },
      {
        "formula_title": "Fractional Exponent Rule (1/x)",
        "description": "Expresses the xth root of a number.",
        "formula": "a^{\\frac{1}{x}} = \\sqrt[x]{a}",
        "key": "a: base, x: root"
      },
      {
        "formula_title": "Division Rule",
        "description": "Division of different bases raised to the same exponent.",
        "formula": "\\frac{a^m}{b^m} = \\left(\\frac{a}{b}\\right)^m",
        "key": "a, b: bases, m: exponent"
      }
    ]
  },
  
    {
      "tableTitle": "Definition of Logarithm",
      "rows": [
        {
          "formula_title": "Basic Definition",
          "description": "The logarithm of a number with respect to a base.",
          "formula": "\\log_b a = c \\text{ means } b^c = a",
          "key": "b: base, a: argument, c: logarithm result"
        }
      ]
    },
    {
      "tableTitle": "Basic Logarithmic Properties",
      "rows": [
        {
          "formula_title": "Product Rule",
          "description": "Logarithm of a product.",
          "formula": "\\log_b(xy) = \\log_b x + \\log_b y",
          "key": "x, y: arguments of the logarithms"
        },
        {
          "formula_title": "Quotient Rule",
          "description": "Logarithm of a quotient.",
          "formula": "\\log_b\\left(\\frac{x}{y}\\right) = \\log_b x - \\log_b y",
          "key": "x: numerator, y: denominator"
        },
        {
          "formula_title": "Power Rule",
          "description": "Logarithm of a power.",
          "formula": "\\log_b(x^y) = y \\log_b x",
          "key": "x: base, y: exponent"
        }
      ]
    },
    {
      "tableTitle": "Change of Base Formula",
      "rows": [
        {
          "formula_title": "Change of Base",
          "description": "Converting logarithms to a different base.",
          "formula": "\\log_b a = \\frac{\\log_k a}{\\log_k b}",
          "key": "k: new base"
        }
      ]
    },
    {
      "tableTitle": "Special Bases",
      "rows": [
        {
          "formula_title": "Natural Logarithm",
          "description": "Property of the natural logarithm.",
          "formula": "\\ln e = 1",
          "key": "e: Euler's number"
        },
        {
          "formula_title": "Common Logarithm",
          "description": "Property of the common logarithm.",
          "formula": "\\log 10 = 1",
          "key": "Logarithm with base 10"
        }
      ]
    },
    {
      "tableTitle": "Inverse Properties",
      "rows": [
        {
          "formula_title": "Inverse of Logarithm",
          "description": "Inverse relationship between exponentiation and logarithms.",
          "formula": "b^{\\log_b x} = x",
          "key": "b: base, x: exponent/logarithm argument"
        },
        {
          "formula_title": "Inverse of Exponentiation",
          "description": "Logarithm as the inverse of exponentiation.",
          "formula": "\\log_b(b^x) = x",
          "key": "b: base, x: exponent"
        }
      ]
    },
    {
      "tableTitle": "Logarithm of 1 and 0",
      "rows": [
        {
          "formula_title": "Logarithm of 1",
          "description": "Logarithmic value of 1.",
          "formula": "\\log_b 1 = 0",
          "key": "b: base"
        },
        {
          "formula_title": "Logarithm of 0",
          "description": "Logarithmic value of 0 is undefined.",
          "formula": "\\log_b 0 \\text{ is undefined}",
          "key": "b: base"
        }
      ]
    },
    {
      "tableTitle": "Logarithmic Identities",
      "rows": [
        {
          "formula_title": "Identity with Exponents",
          "description": "Logarithm of a number raised to an exponent.",
          "formula": "\\ln(x^y) = y\\ln x",
          "key": "x: base, y: exponent"
        },
        {
          "formula_title": "Logarithm of Base",
          "description": "Logarithm where the number is the base itself.",
