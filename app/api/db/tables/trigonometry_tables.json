    "tableTitle": "Double-Angle Identities",
    "rows": [
      {
        "formula_title": "Sine Double-Angle",
        "description": "Sine of double the angle.",
        "formula": "\\sin(2\\theta) = 2\\sin(\\theta)\\cos(\\theta) = (\\sin(\\theta) + \\cos(\\theta))^2 - 1 = \\frac{2\\tan(\\theta)}{1 + \\tan^2(\\theta)}"
        
      },
      {
        "formula_title": "Cosine Double-Angle",
        "description": "Cosine of double the angle.",
        "formula": "\\cos(2\\theta) = \\cos^2(\\theta) - \\sin^2(\\theta) = 2\\cos^2(\\theta) - 1 = 1 - 2\\sin^2(\\theta) = \\frac{1 - \\tan^2(\\theta)}{1 + \\tan^2(\\theta)}"
        
      },
      {
        "formula_title": "Tangent Double-Angle",
        "description": "Tangent of double the angle.",
        "formula": "\\tan(2\\theta) = \\frac{2\\tan(\\theta)}{1 - \\tan^2(\\theta)}"
       
      },
      {
        "formula_title": "Cotangent Double-Angle",
        "description": "Cotangent of double the angle.",
        "formula": "\\cot(2\\theta) = \\frac{\\cot^2(\\theta) - 1}{2\\cot(\\theta)} = \\frac{1 - \\tan^2(\\theta)}{2\\tan(\\theta)}"
        
      },
      {
        "formula_title": "Secant Double-Angle",
        "description": "Secant of double the angle.",
        "formula": "\\sec(2\\theta) = \\frac{\\sec^2(\\theta)}{2 - \\sec^2(\\theta)} = \\frac{1 + \\tan^2(\\theta)}{1 - \\tan^2(\\theta)}"
       
      },
      {
        "formula_title": "Cosecant Double-Angle",
        "description": "Cosecant of double the angle.",
        "formula": "\\csc(2\\theta) = \\frac{\\sec(\\theta)\\csc(\\theta)}{2} = \\frac{1 + \\tan^2(\\theta)}{2\\tan(\\theta)}"
        
      }
    ]
  }
