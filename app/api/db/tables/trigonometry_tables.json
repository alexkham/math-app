  {
    "tableTitle": "Half-Angle Identities",
    "rows": [
      {
        "formula_title": "Sine Half-Angle",
        "description": "Sine of half the angle.",
        "formula": "\\sin\\left(\\frac{\\theta}{2}\\right) = \\operatorname{sgn}\\left(\\sin\\frac{\\theta}{2}\\right)\\sqrt{\\frac{1 - \\cos(\\theta)}{2}}"
        
      },
      {
        "formula_title": "Cosine Half-Angle",
        "description": "Cosine of half the angle.",
        "formula": "\\cos\\left(\\frac{\\theta}{2}\\right) = \\operatorname{sgn}\\left(\\cos\\frac{\\theta}{2}\\right)\\sqrt{\\frac{1 + \\cos(\\theta)}{2}}"
        
      },
      {
        "formula_title": "Tangent Half-Angle",
        "description": "Tangent of half the angle.",
        "formula": "\\tan\\left(\\frac{\\theta}{2}\\right) = \\frac{1 - \\cos(\\theta)}{\\sin(\\theta)} = \\frac{\\sin(\\theta)}{1 + \\cos(\\theta)} = \\csc(\\theta) - \\cot(\\theta) = \\frac{\\tan(\\theta)}{1 + \\sec(\\theta)} = \\operatorname{sgn}(\\sin(\\theta))\\sqrt{\\frac{1 - \\cos(\\theta)}{1 + \\cos(\\theta)}} = \\frac{-1 + \\operatorname{sgn}(\\cos(\\theta))\\sqrt{1 + \\tan^2(\\theta)}}{\\tan(\\theta)}"
        
      },
      {
        "formula_title": "Cotangent Half-Angle",
        "description": "Cotangent of half the angle.",
        "formula": "\\cot\\left(\\frac{\\theta}{2}\\right) = \\frac{1 + \\cos(\\theta)}{\\sin(\\theta)} = \\frac{\\sin(\\theta)}{1 - \\cos(\\theta)} = \\csc(\\theta) + \\cot(\\theta) = \\operatorname{sgn}(\\sin(\\theta))\\sqrt{\\frac{1 + \\cos(\\theta)}{1 - \\cos(\\theta)}}"
       
      },
      {
        "formula_title": "Secant Half-Angle",
        "description": "Secant of half the angle.",
        "formula": "\\sec\\left(\\frac{\\theta}{2}\\right) = \\operatorname{sgn}\\left(\\cos\\frac{\\theta}{2}\\right)\\sqrt{\\frac{2}{1 + \\cos(\\theta)}}"
        
      },
      {
        "formula_title": "Cosecant Half-Angle",
        "description": "Cosecant of half the angle.",
        "formula": "\\csc\\left(\\frac{\\theta}{2}\\right) = \\operatorname{sgn}\\left(\\sin\\frac{\\theta}{2}\\right)\\sqrt{\\frac{2}{1 - \\cos(\\theta)}}"
       
      }
    ]
