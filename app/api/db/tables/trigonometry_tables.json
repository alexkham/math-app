    "tableTitle": "Triple-Angle Identities",
    "rows": [
      {
        "formula_title": "Sine Triple-Angle",
        "description": "Sine of triple the angle.",
        "formula": "\\sin(3\\theta) = 3\\sin(\\theta) - 4\\sin^3(\\theta) = 4\\sin(\\theta)\\sin\\left(\\frac{\\pi}{3} - \\theta\\right)\\sin\\left(\\frac{\\pi}{3} + \\theta\\right)"
        
      },
      {
        "formula_title": "Cosine Triple-Angle",
        "description": "Cosine of triple the angle.",
        "formula": "\\cos(3\\theta) = 4\\cos^3(\\theta) - 3\\cos(\\theta) = 4\\cos(\\theta)\\cos\\left(\\frac{\\pi}{3} - \\theta\\right)\\cos\\left(\\frac{\\pi}{3} + \\theta\\right)"
        
      },
      {
        "formula_title": "Tangent Triple-Angle",
        "description": "Tangent of triple the angle.",
        "formula": "\\tan(3\\theta) = \\frac{3\\tan(\\theta) - \\tan^3(\\theta)}{1 - 3\\tan^2(\\theta)} = \\tan(\\theta)\\tan\\left(\\frac{\\pi}{3} - \\theta\\right)\\tan\\left(\\frac{\\pi}{3} + \\theta\\right)"
        
      },
      {
        "formula_title": "Cotangent Triple-Angle",
        "description": "Cotangent of triple the angle.",
        "formula": "\\cot(3\\theta) = \\frac{3\\cot(\\theta) - \\cot^3(\\theta)}{1 - 3\\cot^2(\\theta)}"
       
      },
      {
        "formula_title": "Secant Triple-Angle",
        "description": "Secant of triple the angle.",
        "formula": "\\sec(3\\theta) = \\frac{\\sec^3(\\theta)}{4 - 3\\sec^2(\\theta)}"
        
      },
      {
        "formula_title": "Cosecant Triple-Angle",
        "description": "Cosecant of triple the angle.",
        "formula": "\\csc(3\\theta) = \\frac{\\csc^3(\\theta)}{3\\csc^2(\\theta) - 4}"
        
      }
    ]
  }
  
