    "tableTitle": "Trigonometric Function Transformations",
    "rows": [
      {
        "Shift by one quarter period": "sin(θ ± π/2) = ± cos θ",
        "Shift by one half period": "sin(θ + π) = -sin θ",
        "Shift by full periods": "sin(θ + k · 2π) = +sin θ",
        "Period": "2π"
      },
