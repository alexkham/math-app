    "rows": [
      {
        "formula_title": "Basic Pythagorean Identity",
        "description": "Fundamental relationship between sine and cosine of the same angle.",
        "formula": "\\sin^2(\\theta) + \\cos^2(\\theta) = 1"
        
      },
