  {
    "tableTitle": "Angle Sum Difference  Formulas",
    "rows": [
      {
        "Function": "Sine",
        "Identity": "sin(α ± β) = sin α cos β ± cos α sin β"
      },
      {
        "Function": "Cosine",
        "Identity": "cos(α ± β) = cos α cos β ∓ sin α sin β"
      },
      {
        "Function": "Tangent",
        "Identity": "tan(α ± β) = (tan α ± tan β) / (1 ∓ tan α tan β)"
      },
      {
        "Function": "Cosecant",
        "Identity": "csc(α ± β) = (sec α sec β csc α csc β) / (sec α csc β ± csc α sec β)"
      },
      {
        "Function": "Secant",
        "Identity": "sec(α ± β) = (sec α sec β csc α csc β) / (csc α csc β ∓ sec α sec β)"
      },
      {
        "Function": "Cotangent",
        "Identity": "cot(α ± β) = (cot α cot β ∓ 1) / (cot β ± cot α)"
      },
      {
        "Function": "Arcsine",
        "Identity": "arcsin x ± arcsin y = arcsin(x√(1-y²) ± y√(1-x²))"
      },
      {
        "Function": "Arccosine",
        "Identity": "arccos x ± arccos y = arccos(xy ∓ √((1-x²)(1-y²)))"
      },
      {
        "Function": "Arctangent",
        "Identity": "arctan x ± arctan y = arctan((x ± y) / (1 ∓ xy))"
      },
      {
        "Function": "Arccotangent",
        "Identity": "arccot x ± arccot y = arccot((xy ∓ 1) / (y ± x))"
      }
    ]
  },
